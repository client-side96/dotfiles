. "$HOME/.cargo/env"

export EDITOR='hx'
export HELIX_RUNTIME=$HOME/pkg/helix/runtime

export PATH=/usr/local/flutter/bin:$HOME/fvm/default/bin:$GEM_HOME/bin:$HOME/.gem/ruby/2.6.0/bin:$HOME/.pub-cache/bin:$PATH

export PYENV_ROOT="$HOME/.pyenv"
[[ -d $PYENV_ROOT/bin ]] && export PATH="$PYENV_ROOT/bin:$PATH"
